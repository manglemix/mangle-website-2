{
	"name": "mangle-website-2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"wasm": "wasm-pack build ./src/hero-wasm --target web",
		"dev": "npm run wasm && vite dev",
		"build": "npm run wasm && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.4.4",
		"@sveltejs/enhanced-img": "^0.3.8",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"fs": "^0.0.1-security",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.8",
		"hero-wasm": "./src/hero-wasm/pkg"
	},
	"type": "module",
	"dependencies": {
		"svelte-highlight": "^7.7.0"
	}
